<Window x:Class="AttachToChrome.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AttachToChrome"
        xmlns:ViewModel="clr-namespace:AttachToChrome.ViewModel"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.DataContext>
        <ViewModel:MainViewModel />
    </Window.DataContext>
    <Grid>
        <StackPanel VerticalAlignment="Center">
            <TextBlock Text="Hello World!"/>
            <Button Content="Launch Browser" Command="{Binding LaunchBrowser}" AutomationProperties.AutomationId="launchBrowserButton"/>
        </StackPanel>
    </Grid>
</Window>
